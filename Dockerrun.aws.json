{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "frontend",
        "image": "zinalacina/sisflood-frontend",
        "hostname": "frontend",
        "essential": false,
        "memory": 128
      },
      {
        "name": "nginx",
        "image": "zinalacina/sisflood-nginx",
        "hostname": "nginx",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "links": ["frontend"],
        "memory": 128
      }
    ]
  }
  